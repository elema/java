import java.io.File;
import java.io.FileReader;


public class App {

	public static void main(String[] args) {
		System.out.println("Chapter 34 = Exceptions");
		File file = new File("./src/example.txt");
		try {
		FileReader fileReader = new FileReader(file);
		} throws (Exception e){
			e.getMessage();
		}
		fileReader.close();
		
	}

}
