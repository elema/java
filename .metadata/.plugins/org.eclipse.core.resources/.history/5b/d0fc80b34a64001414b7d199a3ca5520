package Equals;

class Person{
	private int id;
	private String name;
	public Person(int id, String name) {
		this.id = id;
		this.name = name;
	}
}

public class App {

	public static void main(String[] args) {
		System.out.println("Chapter 42 - Equals()");
		Person ak = new Person(1,"asim");
		System.out.println(ak);
	}

}
